<?xml version='1.0' encoding='UTF-8'?>

<templates>
	<template abbreviation="jscript" description="javascript tag" context="HTML" code="&lt;script language=&quot;JavaScript&quot; type=&quot;text/javascript&quot;&gt;&#10;&#9;${END}&#10;&lt;/script&gt;" isUserTemplate="false"/>
	<template abbreviation="form" description="create a get form" context="HTML" code="&lt;form action=&quot;${action}&quot; method=&quot;${get}&quot;&gt;&#10;${END}&#10;&lt;/form&gt;" isUserTemplate="false"/>
	<template abbreviation="inps" description="input by password field" context="HTML" code="&lt;input type=&quot;password&quot; name=&quot;${name}&quot; size=&quot;40&quot; maxlength=&quot;40&quot;/&gt;  ${END}" isUserTemplate="false"/>
	<template abbreviation="inra" description="input by radio button" context="HTML" code="&lt;input type=&quot;radio&quot; name=&quot;${name}&quot; value=&quot;${value}&quot;/&gt;  ${END}" isUserTemplate="false"/>
	<template abbreviation="formp" description="create a post form" context="HTML" code="&lt;form action=&quot;${action}&quot; method=&quot;${post}&quot; enctype=&quot;text/plain&quot;&gt;&#10;${END}&#10;&lt;/form&gt;" isUserTemplate="false"/>
	<template abbreviation="inbu" description="input by button" context="HTML" code="&lt;input type=&quot;button&quot; name=&quot;${name}&quot; value=&quot;${value}&quot;/&gt;  ${END}" isUserTemplate="false"/>
	<template abbreviation="ta" description="form textarea" context="HTML" code="&lt;textarea name=&quot;${name}&quot; rows=&quot;10&quot; cols=&quot;50&quot; wrap=&quot;off&quot;&gt;&#10;&#9;${text}&#10;&lt;/textarea&gt;${END}" isUserTemplate="false"/>
	<template abbreviation="html" description="create html page" context="HTML" code="&lt;html&gt;&#10;&lt;head&gt;&#10;&lt;meta http-equiv=&quot;Content-Language&quot; content=&quot;en&quot; /&gt;&#10;&lt;meta name=&quot;GENERATOR&quot; content=&quot;Zend Studio&quot; /&gt;&#10;&lt;meta http-equiv=&quot;Content-Type&quot; content=&quot;text/html; charset=iso-8859-1&quot; /&gt;&#10;&lt;title&gt;${title}&lt;/title&gt;&#10;&lt;/head&gt;&#10;&lt;body bgcolor=&quot;#FFFFFF&quot; text=&quot;#000000&quot; link=&quot;#FF9966&quot; vlink=&quot;#FF9966&quot; alink=&quot;#FFCC99&quot;&gt;&#10;${END}&#10;&lt;/body&gt;&#10;&lt;/html&gt;" isUserTemplate="false"/>
	<template abbreviation="ol" description="ordered list" context="HTML" code="&lt;ol&gt;&#10;&#9;&lt;li&gt;${item1}&lt;br/&gt;&#10;&#9;&lt;/li&gt;&#10;&#9;&lt;li&gt;${item2}&lt;br/&gt;&#10;&#9;&lt;/li&gt;&#10;&lt;/ol&gt;${END}" isUserTemplate="false"/>
	<template abbreviation="ul" description="bulleted list" context="HTML" code="&lt;ul&gt;&#10;&#9;&lt;li&gt;${item 1}&lt;br/&gt;&#10;&#9;&lt;/li&gt;&#10;&#9;&lt;li&gt;${item 2}&lt;br/&gt;&#10;&#9;&lt;/li&gt;&#10;&lt;/ul&gt;${END}" isUserTemplate="false"/>
	<template abbreviation="insub" description="input type submit" context="HTML" code="&lt;input type=&quot;submit&quot; name=&quot;${name}&quot; value=&quot;${value}&quot;/&gt;  ${END}" isUserTemplate="false"/>
	<template abbreviation="ihref" description="create an image hyperlink" context="HTML" code="&lt;a href=&quot;${link text}&quot;&gt;&lt;img src=&quot;image&quot;&gt;&lt;/a&gt; ${END}" isUserTemplate="false"/>
	<template abbreviation="htable" description="table 3 rows by 3 columns with header" context="HTML" code="&lt;table&gt;&#10;&#9;&lt;tr&gt;&#10;&#9;&#9;&lt;th&gt;${header 0}&lt;/th&gt;&lt;th&gt;${header 1}&lt;/th&gt;&lt;th&gt;${header 2}&lt;/th&gt;&#10;&#9;&lt;/tr&gt;&#10;&#9;&lt;tr&gt;&#10;&#9;&#9;&lt;td&gt;${position 0,0}&lt;/td&gt;&lt;td&gt;${position 0,1}&lt;/td&gt;&lt;td&gt;${position 0,2}&lt;/td&gt;&#10;&#9;&lt;/tr&gt;&#10;&#9;&lt;tr&gt;&#10;&#9;&#9;&lt;td&gt;${position 1,0}&lt;/td&gt;&lt;td&gt;${position 1,1}&lt;/td&gt;&lt;td&gt;${position 1,2}&lt;/td&gt;&#10;&#9;&lt;/tr&gt;&#10;&#9;&lt;tr&gt;&#10;&#9;&#9;&lt;td&gt;${position 2,0}&lt;/td&gt;&lt;td&gt;${position 2,1}&lt;/td&gt;&lt;td&gt;${position 2,2}&lt;/td&gt;&#10;&#9;&lt;/tr&gt;&#10;&lt;/table&gt;${END}" isUserTemplate="false"/>
	<template abbreviation="select" description="select in form" context="HTML" code="&lt;select name=&quot;${name}&quot; size=&quot;3&quot;&gt;&#10;&#9;&lt;option value=&quot;${value1}&quot;&gt;${option1}&lt;/option&gt;&#10;&#9;&lt;option value=&quot;${value2}&quot;&gt;${option2}&lt;/option&gt;&#10;&#9;&lt;option value=&quot;${value3}&quot;&gt;${option3}&lt;/option&gt;&#10;&lt;/select&gt;${END}" isUserTemplate="false"/>
	<template abbreviation="intxt" description="input by text field" context="HTML" code="&lt;input type=&quot;text&quot; name=&quot;${name}&quot; value=&quot;${value}&quot;/&gt;  ${END}" isUserTemplate="false"/>
	<template abbreviation="href" description="create a hyperlink" context="HTML" code="&lt;a href=&quot;${link}&quot;&gt;${link text}&lt;/a&gt; ${END}" isUserTemplate="false"/>
	<template abbreviation="style" description="text/css style" context="HTML" code="&lt;/style type=&quot;text/css&quot;&gt;&#10;&#9;${END}&#10;&lt;/style&gt;" isUserTemplate="false"/>
	<template abbreviation="table" description="table 3 rows by 3 columns" context="HTML" code="&lt;table&gt;&#10;&#9;&lt;tr&gt;&#10;&#9;&#9;&lt;td&gt;${position 0,0}&lt;/td&gt;&lt;td&gt;${position 0,1}&lt;/td&gt;&lt;td&gt;${position 0,2}&lt;/td&gt;&#10;&#9;&lt;/tr&gt;&#10;&#9;&lt;tr&gt;&#10;&#9;&#9;&lt;td&gt;${position 1,0}&lt;/td&gt;&lt;td&gt;${position 1,1}&lt;/td&gt;&lt;td&gt;${position 1,2}&lt;/td&gt;&#10;&#9;&lt;/tr&gt;&#10;&#9;&lt;tr&gt;&#10;&#9;&#9;&lt;td&gt;${position 2,0}&lt;/td&gt;&lt;td&gt;${position 2,1}&lt;/td&gt;&lt;td&gt;${position 2,2}&lt;/td&gt;&#10;&#9;&lt;/tr&gt;&#10;&lt;/table&gt;${END}" isUserTemplate="false"/>
	<template abbreviation="inrst" description="input type reset" context="HTML" code="&lt;input type=&quot;reset&quot; name=&quot;${name}&quot; value=&quot;${value}&quot;/&gt;  ${END}" isUserTemplate="false"/>
	<template abbreviation="inch" description="input by checkbox" context="HTML" code="&lt;input type=&quot;checkbox&quot; name=&quot;${name}&quot; value=&quot;${value}&quot;/&gt;  ${END}" isUserTemplate="false"/>
</templates>
